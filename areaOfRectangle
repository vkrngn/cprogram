#include <stdio.h>
int main()
{
    int length;
    int breadth;
    int area;

    printf("Enter the length of a rectangle : \n");
    scanf("%d", &length);

    printf("Enter the breath of a rectangle : \n");
    scanf("%d", &breadth);

    area = length * breadth;

    printf("The area of a rectangle : %d \n", area);

    return 0;
}
